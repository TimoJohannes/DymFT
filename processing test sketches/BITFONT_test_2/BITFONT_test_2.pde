void drawChar(int x, int y, int c, int size) {
  c-= 32;


  // GFXglyph *glyph  = &(((GFXglyph *)pgm_read_pointer(&gfxFont->glyph))[c]);
  //uint8_t  *bitmap = (uint8_t *)pgm_read_pointer(&gfxFont->bitmap);

  int  bo = fontGlyphs[c][0];//pgm_read_word(&glyph->bitmapOffset);
  int w  = fontGlyphs[c][1];//pgm_read_byte(&glyph->width),
  int h  = fontGlyphs[c][2];
  int   xo = fontGlyphs[c][3];
  int  yo = fontGlyphs[c][4];
  int  xx, yy, bits = 0, bit = 0;
  int byteAddr = 0;
  
  
  
  for (xx=0; xx<w; xx++) {
    for (yy=0; yy<h; yy++) {

      bit = xx+((h-yy-1)*w);

      byteAddr = bo+(bit/8);
      bit = bit%8;
      
  println(yy,h-yy-1);
      bit = 128>>bit;
      if ((font[byteAddr]&bit)!= 0) {
        fill(255);
        rect(x+(xx)*size, y+(yy)*size, 
          size, size);
      }
      

      bits <<= 1;
    }
  }
}


